{
  "name": "rapid-cmi5-electron",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/rapid-cmi5-electron/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx-electron:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "externalDependencies": "none",
        "outputPath": "dist/apps/rapid-cmi5-electron",
        "main": "apps/rapid-cmi5-electron/src/main.ts",
        "tsConfig": "apps/rapid-cmi5-electron/tsconfig.app.json",
        "assets": ["apps/rapid-cmi5-electron/src/assets"]
      },
      "configurations": {
        "production": {
          "baseHref": "./",
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/rapid-cmi5-electron/src/environments/environment.ts",
              "with": "apps/rapid-cmi5-electron/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "nx-electron:execute",
      "options": {
        "buildTarget": "rapid-cmi5-electron:build"
      }
    },
    "package": {
      "executor": "nx-electron:package",
      "options": {
        "name": "rapid-cmi5-electron",
        "frontendProject": "rapid-cmi5-electron-frontend",
        "sourcePath": "dist/apps",
        "outputPath": "dist/packages",
        "prepackageOnly": true,
        "asar": {
          "smartUnpack": true
        },
        "extraResources": [
          {
            "from": "dist/apps/rapid-cmi5-electron/assets",
            "to": "assets",
            "filter": ["**/*"]
          }
        ]
      }
    },
    "make": {
      "executor": "nx-electron:make",
      "options": {
        "name": "rapid-cmi5-electron",
        "frontendProject": "rapid-cmi5-electron-frontend",
        "sourcePath": "dist/apps",
        "outputPath": "dist/executables",
        "asar": {
          "smartUnpack": true
        },
        "executableName": "RapidCMI5",
        "extraResources": [
          {
            "from": "dist/apps/rapid-cmi5-electron/assets",
            "to": "assets",
            "filter": ["**/*"]
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/rapid-cmi5-electron/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/rapid-cmi5-electron/jest.config.ts"
      }
    },
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": ["{workspaceRoot}/dist/.playwright/apps/rapid-cmi5-electron"],
      "options": {
        "config": "apps/rapid-cmi5-electron/playwright.config.ts"
      }
    }
  }
}
