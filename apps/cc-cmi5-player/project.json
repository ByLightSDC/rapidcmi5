{
  "name": "cc-cmi5-player",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cc-cmi5-player/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/cc-cmi5-player",
        "index": "apps/cc-cmi5-player/src/index.html",
        "main": "apps/cc-cmi5-player/src/main.tsx",
        "tsConfig": "apps/cc-cmi5-player/tsconfig.app.json",
        "assets": [
          "apps/cc-cmi5-player/src/favicon.ico",
          "apps/cc-cmi5-player/src/assets",
          "apps/cc-cmi5-player/src/cfg.json",
          {
            "input": "packages/ui/src/assets/rapid-cmi5",
            "glob": "**/*",
            "output": "assets/rapid-cmi5"
          },
          {
            "input": "cmi5-test-lessons/test-lesson/",
            "glob": "**/*",
            "output": "."
          },
          "apps/cc-cmi5-player/src/mockServiceWorker.js"
        ],
        "styles": ["apps/cc-cmi5-player/src/styles.css"],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "apps/cc-cmi5-player/webpack.config.js",
        "postcssConfig": "apps/cc-cmi5-player/postcss.config.js",
        "memoryLimit": 4096
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/cc-cmi5-player/src/environments/environment.ts",
              "with": "apps/cc-cmi5-player/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "cc-cmi5-player:build",
        "hmr": true,
        "port": 4300
      },
      "configurations": {
        "development": {
          "buildTarget": "cc-cmi5-player:build:development"
        },
        "production": {
          "buildTarget": "cc-cmi5-player:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "cc-cmi5-player:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/cc-cmi5-player/jest.config.ts"
      }
    }
  }
}
