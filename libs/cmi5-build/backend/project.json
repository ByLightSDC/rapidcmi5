{
  "name": "cmi5-build-backend",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/cmi5-build/backend/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/cmi5-build/backend",
        "main": "libs/cmi5-build/backend/src/index.ts",
        "tsConfig": "libs/cmi5-build/backend/tsconfig.lib.json",
        "assets": ["libs/cmi5-build/backend/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/cmi5-build/backend/jest.config.ts"
      }
    }
  }
}
