{
  "name": "rapid-cmi5",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/rapid-cmi5/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/rapid-cmi5",
        "main": "libs/rapid-cmi5/src/index.ts",
        "tsConfig": "libs/rapid-cmi5/tsconfig.lib.json",
        "package.json": "libs/rapid-cmi5/package.json",
        "generatePackageJson": true,
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/rapid-cmi5/jest.config.ts"
      }
    }
  }
}