{
  "name": "frontend-ui-snack",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/ui/snack/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/frontend/ui/snack",
        "tsConfig": "libs/frontend/ui/snack/tsconfig.lib.json",
        "project": "libs/frontend/ui/snack/package.json",
        "entryFile": "libs/frontend/ui/snack/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/frontend/ui/snack/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "babelUpwardRootMode": true,
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/frontend/ui/snack"],
      "options": {
        "jestConfig": "libs/frontend/ui/snack/jest.config.ts"
      }
    }
  }
}
