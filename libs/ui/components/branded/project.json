{
  "name": "ui-components-branded",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui/components/branded/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "main": "libs/ui/components/branded/src/index.ts",
        "outputPath": "dist/libs/ui/components/branded",
        "tsConfig": "libs/ui/components/branded/tsconfig.lib.json",
        "package.json": "libs/ui/components/branded/package.json"

      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/ui/components/branded"
      ],
      "options": {
        "jestConfig": "libs/ui/components/branded/jest.config.ts"
      }
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/ui/components/branded/.storybook",
        "assets": [
          {
            "input": "libs/ui/components/branded/src/assets/devops-portal",
            "glob": "**/*",
            "output": "assets/devops-portal"
          }
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": [
        "{options.outputDir}"
      ],
      "options": {
        "outputDir": "dist/storybook/ui-components-branded",
        "configDir": "libs/ui/components/branded/.storybook",
        "assets": [
          {
            "input": "libs/ui/components/branded/src/assets/devops-portal",
            "glob": "**/*",
            "output": "assets/devops-portal"
          }
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c libs/ui/components/branded/.storybook --url=http://localhost:4400"
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "dependsOn": [
        "build-storybook"
      ],
      "options": {
        "buildTarget": "ui-components-branded:build-storybook",
        "staticFilePath": "dist/storybook/ui-components-branded",
        "spa": true
      },
      "configurations": {
        "ci": {
          "buildTarget": "ui-components-branded:build-storybook:ci"
        }
      }
    }
  }
}