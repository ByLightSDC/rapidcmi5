{
  "name": "types-cmi5",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/types/cmi5/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/types/cmi5",
        "tsConfig": "libs/types/cmi5/tsconfig.lib.json",
        "package.json": "libs/types/cmi5/package.json",
        "main": "libs/types/cmi5/src/index.ts",
        "assets": [
          {
            "glob": "libs/types/cmi5/README.md",
            "input": ".",
            "output": "."
          },
          {
            "glob": "**/*.json",
            "input": "libs/types/cmi5/src/schemas",
            "output": "src/schemas"
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/types/cmi5"],
      "options": {
        "jestConfig": "libs/types/cmi5/jest.config.ts"
      }
    }
  }
}
