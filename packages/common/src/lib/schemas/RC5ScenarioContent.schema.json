{
  "$ref": "#/definitions/RC5ScenarioContent",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "KSATElement": {
      "additionalProperties": false,
      "properties": {
        "doc_identifier": {
          "type": "string"
        },
        "element_identifier": {
          "type": "string"
        },
        "element_type": {
          "enum": [
            "task",
            "knowledge",
            "skill"
          ],
          "type": "string"
        },
        "text": {
          "type": "string"
        },
        "title": {
          "type": "string"
        }
      },
      "required": [
        "element_identifier",
        "element_type",
        "title",
        "text",
        "doc_identifier"
      ],
      "type": "object"
    },
    "MoveOnCriteriaEnum": {
      "enum": [
        "completed",
        "passed",
        "completed-and-passed",
        "not-applicable"
      ],
      "type": "string"
    },
    "RC5ScenarioContent": {
      "additionalProperties": false,
      "description": "New data type for RC5 will replace ScenarioContent above",
      "properties": {
        "cmi5QuizId": {
          "type": "string"
        },
        "ksats": {
          "items": {
            "$ref": "#/definitions/KSATElement"
          },
          "type": "array"
        },
        "moveOnCriteria": {
          "$ref": "#/definitions/MoveOnCriteriaEnum"
        },
        "name": {
          "type": "string"
        },
        "promptClass": {
          "type": "boolean"
        },
        "rc5id": {
          "type": "string"
        },
        "uuid": {
          "type": "string"
        }
      },
      "required": [
        "uuid"
      ],
      "type": "object"
    }
  }
}