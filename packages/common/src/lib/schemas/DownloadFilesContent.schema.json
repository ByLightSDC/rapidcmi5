{
  "$ref": "#/definitions/DownloadFilesContent",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "DownloadFileData": {
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "path",
        "type"
      ],
      "type": "object"
    },
    "DownloadFilesContent": {
      "additionalProperties": false,
      "properties": {
        "files": {
          "items": {
            "$ref": "#/definitions/DownloadFileData"
          },
          "type": "array"
        },
        "ksats": {
          "items": {
            "$ref": "#/definitions/KSATElement"
          },
          "type": "array"
        },
        "moveOnCriteria": {
          "$ref": "#/definitions/MoveOnCriteriaEnum"
        },
        "rc5id": {
          "type": "string"
        }
      },
      "required": [
        "files"
      ],
      "type": "object"
    },
    "KSATElement": {
      "additionalProperties": false,
      "properties": {
        "doc_identifier": {
          "type": "string"
        },
        "element_identifier": {
          "type": "string"
        },
        "element_type": {
          "enum": [
            "task",
            "knowledge",
            "skill"
          ],
          "type": "string"
        },
        "text": {
          "type": "string"
        },
        "title": {
          "type": "string"
        }
      },
      "required": [
        "element_identifier",
        "element_type",
        "title",
        "text",
        "doc_identifier"
      ],
      "type": "object"
    },
    "MoveOnCriteriaEnum": {
      "enum": [
        "completed",
        "passed",
        "completed-and-passed",
        "not-applicable"
      ],
      "type": "string"
    }
  }
}